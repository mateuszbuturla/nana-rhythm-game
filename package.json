{
  "name": "nana-rhythm-game",
  "version": "2.2.1",
  "description": "Web based rhythm game",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "nana-rhythm-game",
    "productName": "NaNa rhythm game",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "asar": true
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "buildVersion": "1"
  },
  "author": "Mateusz Buturla <2002mateuszbuturla@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.54.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mocha": "^8.2.0",
    "@types/redux-thunk": "^2.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "lodash": "^4.17.20",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.0",
    "spectron": "^13.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateuszbuturla/nana-rhythm-game"
  }
}
